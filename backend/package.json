  "main": "src/index.js",
  "scripts": {
    "start": "nodemon -e ts,graphql -x ts-node -r dotenv/config src/index.ts",
    "debug": "nodemon -e ts,graphql -x ts-node --inspect -r dotenv/config src/index.ts",
    "playground": "graphql playground --port 3001",
    "dev": "npm-run-all --parallel start playground"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.2.1",
    "jsonwebtoken": "8.1.1",
    "prisma-binding": "1.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "dotenv": "5.0.0",
    "graphql-cli": "2.13.0",
    "nodemon": "1.14.12",
    "npm-run-all": "4.1.2",
    "prisma": "1.1.2",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2"
  }
}
