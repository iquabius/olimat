  "main": "src/index.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "nodemon -e ts,graphql -x ts-node -r dotenv/config src/index.ts",
    "debug": "nodemon --config nodemon.debug.json",
    "test": "jest",
    "lint": "tslint 'src/**/*.ts'",
    "lint:fix": "tslint 'src/**/*.ts' --fix && echo \"tslint: no lint errors\"",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "clean:files": "rimraf files",
    "copy:files": "copyfiles -V -u 2 ./prisma/seed_files/**/* ./files"
  },
  "dependencies": {
    "apollo-server-express": "^2.3.1",
    "bcryptjs": "2.4.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-fileupload": "^1.0.0",
    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "jsonwebtoken": "^8.3.0",
    "mime-types": "^2.1.21",
    "mv": "^2.1.1",
    "prisma-client-lib": "^1.23.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.16.0",
    "@types/express-fileupload": "^0.4.0",
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.3.10",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mime-types": "^2.1.0",
    "@types/mv": "^2.1.0",
    "@types/node": "^10.12.15",
    "@types/shortid": "0.0.29",
    "copyfiles": "^2.1.0",
    "dotenv": "^6.1.0",
    "graphql-cli": "^3.0.3",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.14.3",
    "prisma": "^1.23.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.2.2"
  }
}
