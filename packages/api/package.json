{
  "name": "@olimat/api",
  "private": true,
  "version": "0.0.1",
  "license": "AGPL-3.0",
  "author": "Josias Iquabius <iquabius@gmail.com>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "_moduleAliases": {
    "@olimat/api"  : "dist"
  },
  "scripts": {
    "prebuild": "rimraf dist && copyfiles -V -u 1 ./src/schema.graphql ./src/**/*.graphql ./dist",
    "build:clean": "rimraf dist",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "nodemon -e ts,graphql -x ts-node -r dotenv/config src/index.ts",
    "debug": "nodemon --config nodemon.debug.json",
    "clean:files": "rimraf files",
    "copy:files": "copyfiles -V -u 2 ./prisma/seed_files/**/* ./files",
    "graphql:generate-types": "gql-gen --config codegen.yml"
  },
  "dependencies": {
    "apollo-server-express": "2.4.8",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-fileupload": "1.1.4",
    "graphql": "14.3.0",
    "graphql-import": "0.7.1",
    "jsonwebtoken": "8.5.1",
    "mime-types": "2.1.24",
    "mv": "2.1.1",
    "prisma-client-lib": "1.28.5",
    "shortid": "2.2.14"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.16.1",
    "@types/express-fileupload": "0.4.0",
    "@types/graphql": "14.2.0",
    "@types/jsonwebtoken": "8.3.2",
    "@types/mime-types": "2.1.0",
    "@types/mv": "2.1.0",
    "@types/nock": "9.3.1",
    "@types/shortid": "0.0.29",
    "apollo": "2.10.2",
    "apollo-link": "1.2.11",
    "apollo-link-http": "1.5.14",
    "apollo-server-testing": "2.4.8",
    "copyfiles": "2.1.0",
    "dotenv": "8.0.0",
    "graphql-code-generator": "0.16.1",
    "graphql-codegen-typescript-common": "0.16.1",
    "graphql-codegen-typescript-resolvers": "0.16.1",
    "graphql-codegen-typescript-server": "0.16.1",
    "module-alias": "2.2.0",
    "nock": "10.0.6",
    "node-fetch": "2.5.0",
    "nodemon": "1.19.0",
    "prisma": "1.28.5",
    "rimraf": "2.6.3",
    "ts-node": "8.1.0"
  },
  "repository": "github:iquabius/olimat"
}
