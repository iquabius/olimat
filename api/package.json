{
  "name": "@olimat/api",
  "private": true,
  "version": "0.0.1",
  "license": "AGPL-3.0",
  "author": "Josias Iquabius <iquabius@gmail.com>",
  "main": "src/index.ts",
  "scripts": {
    "build": "rimraf dist && tsc && copyfiles -V -u 1 ./src/schema.graphql ./src/**/*.graphql ./dist",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "nodemon -e ts,graphql -x ts-node -r dotenv/config src/index.ts",
    "debug": "nodemon --config nodemon.debug.json",
    "clean:files": "rimraf files",
    "copy:files": "copyfiles -V -u 2 ./prisma/seed_files/**/* ./files",
    "graphql:generate-types": "gql-gen --config codegen.yml"
  },
  "dependencies": {
    "apollo-server-express": "2.3.2",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-fileupload": "1.0.0",
    "graphql": "14.1.1",
    "graphql-import": "0.7.1",
    "jsonwebtoken": "8.4.0",
    "mime-types": "2.1.21",
    "mv": "2.1.1",
    "prisma-client-lib": "1.25.4",
    "shortid": "2.2.14"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.16.1",
    "@types/express-fileupload": "0.4.0",
    "@types/graphql": "14.0.5",
    "@types/jsonwebtoken": "8.3.0",
    "@types/mime-types": "2.1.0",
    "@types/mv": "2.1.0",
    "@types/nock": "9.3.1",
    "@types/shortid": "0.0.29",
    "apollo": "2.4.0",
    "apollo-link": "1.2.6",
    "apollo-link-http": "1.5.9",
    "apollo-server-testing": "2.3.2",
    "copyfiles": "2.1.0",
    "dotenv": "6.2.0",
    "graphql-code-generator": "0.16.0",
    "graphql-codegen-typescript-common": "0.16.0",
    "graphql-codegen-typescript-resolvers": "0.16.0",
    "graphql-codegen-typescript-server": "0.16.0",
    "nock": "10.0.6",
    "node-fetch": "2.3.0",
    "nodemon": "1.18.9",
    "prisma": "1.25.4",
    "rimraf": "2.6.3",
    "ts-node": "8.0.2"
  },
  "repository": "github:iquabius/olimat"
}
